# Database Configuration
# Copy this file to .env and add your actual credentials
DATABASE_URL=postgresql://username:password@host/database?sslmode=require

# Neon PostgreSQL Connection
# Get your connection string from: https://console.neon.tech/
# Format: postgresql://[user]:[password]@[host]/[database]?sslmode=require

# Google Analytics Configuration
# Get your Measurement ID from: https://analytics.google.com/
# Format: G-XXXXXXXXXX (Google Analytics 4 property)
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Google OAuth Configuration (for Admin CMS access)
# Create credentials at: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_CALLBACK_URL=http://localhost:5000/api/auth/google/callback

# Session Configuration
# Generate a strong random secret for production
SESSION_SECRET=change-this-to-a-random-secret-in-production

# Webhook Configuration (for contact form)
WEBHOOK_URL=https://janio.app.n8n.cloud/webhook/ddf4a9ce-40ef-462c-8264-c582a28e3ae2
WEBHOOK_USERNAME=janiowebsiteleadform
WEBHOOK_PASSWORD=your-webhook-password
